  g'4. af8 |
  c,4( b8) c-. |
  d( ef f ef) |
  ef4( d) | %m40
  g4. af8 |
  c,4( b8) c-. |
  f( d c b) |
  b4( c8) g |
  \key c \major
  \repeat volta 2
  {
    \once\override Score.RehearsalMark #'break-align-symbols = #'(key-signature)
