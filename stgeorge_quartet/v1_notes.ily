violinIMII = \relative c'''
{
  \repeat volta 2
  {
    g4.\p af8 |
    c,4( b8) c-. |
    d( ef f ef) |
    ef4( d) |
    g4. af8 | %m5
    c,4( b8) c-. |
    f( d c b) |
    b4( c)\mark "fin" |
  }
  bf4. c8 |
  c bf af g | %m10
  f(\trill e) f-. g-. |
  g4( af8) r |
  af4. bf8 |
  bf( af g) f-. |
  ef( d) ef-. f-. | %m15
  f4( g8) r |
  \repeat unfold 2
  { bf'( bf,) bf-. bf-. | bf-. bf-. bf-. bf-. | }
  bf-. ef( d ef) | %m21
  f,4. c'8 |
  c8( bf) r d, |
  d4( ef8) r |
  c'4.( df8) | %m25
  c4.( df8) |
  c g' c, bf |
  bf4 af |
  af'4. g8 |
  \appoggiatura g f4. af8 | %m30
  ef( d) d( c) |
  b4 g'8( f) |
  f( ef d c) |
  b4 g'8( f) |
  f-. ef-. d-. c-. | %m35
